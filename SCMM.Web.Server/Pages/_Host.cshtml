@page "/"
@namespace SCMM.Web.Server.Pages
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@model HostModel
@{
    var renderMode = (Model.AppState.Runtime == Data.Models.RuntimeType.WebAssembly ? RenderMode.WebAssemblyPrerendered : RenderMode.Server);
    Model.AppState.IsPrerendering = true;
    Layout = "_Layout";
}

<component type="typeof(SCMM.Web.Client.App)" render-mode="@renderMode" />
